{
  "swagger": "2.0",
  "info": {
    "title": "API V1",
    "version": "v1"
  },
  "paths": {
    "/api/info": {
      "get": {
        "summary": "container information",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "success",
            "schema": {
              "type": "object",
              "properties": {
                "records": {
                  "type": "integer"
                }
              },
              "required": [
                "records"
              ]
            }
          }
        }
      }
    },
    "/api/data": {
      "get": {
        "summary": "read data",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "success"
          }
        }
      },
      "post": {
        "summary": "write data",
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "input",
            "in": "body"
          }
        ],
        "responses": {
          "200": {
            "description": "success"
          },
          "500": {
            "description": "not an array"
          }
        }
      }
    },
    "/api/log": {
      "get": {
        "summary": "log information",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "success"
          }
        }
      }
    }
  }
}